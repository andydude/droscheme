(package
  ds_any_runtime
  (import "runtime")
  (func getFE
        (#(pc &uintptr))
        &uintptr
        (return ((dot (runtime.FuncForPC pc) Entry))))
  (func getFL
        (#(pc &uintptr))
        (values #(file &imm-string) #(line &int))
        (return
          ((dot (runtime.FuncForPC pc) FileLine) pc)))
  (func getFN
        (#(pc &uintptr))
        &imm-string
        (return ((dot (runtime.FuncForPC pc) Name)))))
