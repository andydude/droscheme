- List of dependancies

zero? : cond
eq? : eqv?
eqv? : cond
cond : syntax-rules
fold-left : case-lambda
fold-right : case-lambda
case-lambda : syntax-rules
case : syntax-rules
and : syntax-rules
or : syntax-rules
when : syntax-rules
unless : syntax-rules

- Macros

(and #t (begin (display "hello") #t))
(and #t (begin (display "hello") #f))
(and #f (begin (display "hello") #t))
(and #f (begin (display "hello") #f))

(define-macro (and . rest)
  `(if (null? (list ,@rest)) #t
       (if (car (list ,@rest))
           (and ,@(cdr (list ,@rest)))
           #f)))

(define-macro (or . rest)
  `(if (null? (list ,@rest)) #f
       (if (car (list ,@rest))
           #t
           (or ,@(cdr (list ,@rest))))))

